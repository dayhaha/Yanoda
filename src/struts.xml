<?xml version="1.0" encoding="UTF-8"?>
 
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
<constant name="struts.devMode" value="true"></constant>
 
<constant name="struts.convention.result.path" value="/"></constant>
 
<package name="test" namespace="/" extends="struts-default" >
	<action name="login" class="com.action.UserAction" method="login">
	<result name="success">/TestStruts.jsp</result>
	<result name="error">/TestTomcat.jsp</result>
	</action>
	<action name="upload" class="com.action.Upload" method="excute">
	<result>/TestStruts.jsp</result>
	</action>
</package>
 
</struts>
    
